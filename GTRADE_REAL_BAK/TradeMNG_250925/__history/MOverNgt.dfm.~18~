  inherited dbMain: TUniQuery
    SQL.Strings = (
      'SELECT A.USER_ID,'
      'D.USER_NM,'
      'A.ACNT_NO,'
      'A.ARTC_CD,'
      'B.STK_CD,'
      'A.OVERNGT_YN,'
      'D.ACNT_AMT + D.CLR_PL - D.CMSN AS ACNT_AMT,'
      'B.BS_TP,'
      'B.NCLR_POS_QTY,'
      'B.AVG_PRC,'
      'C.CNTR_PRC,'
      'CASE WHEN (A.ARTC_CD = '#39'201'#39' OR A.ARTC_CD = '#39'301'#39') THEN'
      
        '     CASE WHEN B.BS_TP = '#39'S'#39' THEN (B.AVG_PRC - C.CNTR_PRC) * B.N' +
        'CLR_POS_QTY * (E.TICK_VALUE_LOW / E.TICK_SIZE_LOW)'
      
        '     ELSE (C.CNTR_PRC - B.AVG_PRC) * B.NCLR_POS_QTY * (E.TICK_VA' +
        'LUE_LOW / E.TICK_SIZE_LOW) END '
      'ELSE     '
      
        '     CASE WHEN B.BS_TP = '#39'S'#39' THEN (B.AVG_PRC - C.CNTR_PRC) * B.N' +
        'CLR_POS_QTY * (E.TICK_VALUE / E.TICK_SIZE)'
      
        '     ELSE (C.CNTR_PRC - B.AVG_PRC) * B.NCLR_POS_QTY * (E.TICK_VA' +
        'LUE / E.TICK_SIZE) END '
      'END AS EVA_PL'
      'FROM OVERNGT A, NCLR_POS B, CURR_PRC C, ACNT_MST D, ARTC_MST E'
      'WHERE A.ACNT_NO = B.ACNT_NO'
      'AND A.ACNT_NO = D.ACNT_NO'
      'AND A.ARTC_CD = B.ARTC_CD'
      'AND B.STK_CD = C.STK_CD'
      'AND (C.NGT_YN = '#39'A'#39' OR '
      
        '     C.NGT_YN = (SELECT CASE WHEN MKT_TP >= 3 THEN '#39'Y'#39' ELSE '#39'N'#39' ' +
        'END FROM ARTC_MST WHERE ARTC_CD = '#39'101'#39'))'
      'AND A.ARTC_CD = E.ARTC_CD'
      'AND A.OVERNGT_YN = '#39'Y'#39)
    OnCalcFields = dbMainCalcFields
