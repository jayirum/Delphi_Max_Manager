        'P'#39' AND CODE_VALUE = A.BS_TP) AS BS_TP,'
      '       A.REMN_QTY,'
      '       A.ORD_PRC'
      'FROM ORD A,'
      '     ACNT_MST B'
      'WHERE A.ACNT_NO = B.ACNT_NO '
      'AND A.COND_TP <> '#39'1'#39
      'AND A.REMN_QTY > 0'
      'AND A.API_TP = '#39'R'#39
      'ORDER BY B.USER_NM DESC, A.STK_CD DESC')
    FetchRows = 50
    SpecificOptions.Strings = (
      'SQL Server.FetchAll=False')
    Left = 752
    Top = 128
